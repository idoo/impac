define [
  "angular"
  "angular-ui-router"
], (angular) ->

#config path for home page
  angular.module("homeModule", ["ui.router"])
  .config([
      "$stateProvider"
      ($stateProvider) ->
        $stateProvider.state "home",
          url: "/"
          templateUrl: "<%= asset_path("home/templates/home.tpl.html") %>"
          controller: "HomeController"
    ])
  .controller("HomeController", [
      "$scope"
      "$location"

      ($scope, $location) ->
        console.log 'lets start!'
    ])
